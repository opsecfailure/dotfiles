profile nomad {
	output eDP-1 enable
    exec pkill wlsunset; wlsunset -T 2500 -t 2200 -l 41 -L 34
}

profile home {
    output eDP-1 disable
    output "Samsung Electric Company LF24T450F HK2T700667" {
        mode 1920x1080
    }
    exec pkill wlsunset
}

profile docked {
	output eDP-1 disable
	output HDMI-A-1 enable
}
