profile nomad {
	output eDP-1 {
        scale 1.5
    }
    exec pkill wlsunset; wlsunset -t 2200 -T 5500 -l 41 -L 34
}

profile home {
    output eDP-1 disable
    output "Samsung Electric Company LF24T450F HK2T700667" {
        mode 1920x1080
        scale 1.75
    }
    exec pkill wlsunset; wlsunset -t 2700 -T 5500 -l 41 -L 34
}

profile docked {
	output eDP-1 disable
	output HDMI-A-1 enable
}
